<p id="notice"><%= notice %></p>

<h1>Clubs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Open</th>
      <th>Close</th>
      <th>Address</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Phone</th>
      <th>Cover</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clubs.each do |club| %>
      <tr>
        <td><%= club.name %></td>
        <td><%= club.open %></td>
        <td><%= club.close %></td>
        <td><%= club.address %></td>
        <td><%= club.longitude %></td>
        <td><%= club.latitude %></td>
        <td><%= club.phone %></td>
        <td><%= club.cover %></td>
        <td><%= club.content %></td>
        <td><%= link_to 'Show', club %></td>
        <td><%= link_to 'Edit', edit_club_path(club) %></td>
        <td><%= link_to 'Destroy', club, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Club', new_club_path %>
